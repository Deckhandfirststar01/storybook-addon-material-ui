{
  "name": "storybook-addon-material-ui",
  "version": "0.8.5",
  "description": "Storybook Addon for Material Ui Library",
  "main": "dist/index.js",
  "scripts": {
    "deploy": "node .scripts/deployer",
    "prepublish": "node .scripts/npm-prepublish.js",
    "postpublish": "node .scripts/npm-postpublish.js",
    "lint": "node .scripts/lint.js",
    "lintfix": "node .scripts/lint.js --fix",
    "testonly": "node .scripts/run_tests",
    "test": "npm run lint && npm run testonly",
    "test-watch": "node .scripts/run_tests --watch --watch-extensions jsx,json",
    "test-storybook": "storyshots",
    "status": "node .scripts/npm-status.js && node .scripts/lint.js -f json && node .scripts/run_tests -R min",
    "storybook": "start-storybook -p 9010 && npm run test-watch",
    "start": "start-storybook -p 9001"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-theming/storybook-addon-material-ui.git"
  },
  "keywords": [
    "storybook",
    "react",
    "material",
    "ui",
    "material-ui",
    "addon",
    "decorator",
    "theme",
    "editor",
    "customization",
    "dark theme",
    "light theme"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-theming/storybook-addon-material-ui/issues"
  },
  "homepage": "https://github.com/react-themingstorybook-addon-material-ui",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-info": "^3.4.6",
    "@storybook/addon-options": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "@storybook/ui": "^3.4.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "fifteen-kilos": "^2.0.0",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.12.0",
    "material-ui": "^0.20.1",
    "mocha": "^3.5.3",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^16.4.2",
    "shelljs": "^0.7.8",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1"
  },
  "peerDependencies": {
    "@storybook/react": "^3.0.0",
    "material-ui": ">=0.15.0",
    "prop-types": "^15.5.8",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "webpack": ">=2.0.0"
  },
  "dependencies": {
    "@storybook/addons": "^3.4.6",
    "js-beautify": "^1.7.3",
    "react-material-color-picker": "^1.1.2",
    "react-split-pane": "^0.1.66"
  }
}
